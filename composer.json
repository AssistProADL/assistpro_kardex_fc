    "illuminate/console": "^12.40",
    "illuminate/container": "^12.40",
    "illuminate/database": "^12.40",
    "illuminate/events": "^12.40",
    "illuminate/filesystem": "^12.40",
    "illuminate/http": "^12.40",
    "illuminate/pagination": "^12.40",
    "illuminate/routing": "^12.40",
    "illuminate/translation": "^12.40",
    "illuminate/validation": "^12.40",
    "illuminate/view": "^12.40",
    "monolog/monolog": "^2.0",
    "phpmailer/phpmailer": "^7.0",
    "phpoffice/phpspreadsheet": "1.25.2",
    "picqer/php-barcode-generator": "^3.2",
